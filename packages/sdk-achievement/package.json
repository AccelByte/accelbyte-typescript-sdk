{
  "name": "@accelbyte/sdk-achievement",
  "version": "4.0.0-experimental.0",
  "author": "AccelByte Inc",
  "license": "SEE LICENSE IN LICENSE",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --dts --format esm,cjs",
    "test": "echo ''",
    "clean": "npx rimraf dist && npx rimraf node_modules && npx rimraf .yalc",
    "cg:prettify": "prettier --write swaggers/*.json && prettier --write --no-error-on-unmatched-pattern src/**/*",
    "cg:download": "yarn cg download-swaggers --config swaggers.json --swaggersOutput swaggers",
    "cg:generate": "rimraf src/generated-public && yarn cg generate-code --config swaggers.json --swaggersOutput swaggers --output src --snippetOutput ../../../apps/api-explorer-app/src/constants/Snippets",
    "cg": "ts-node ../codegen/src/cli.ts",
    "codegen": "yarn cg:generate && yarn cg:prettify"
  },
  "devDependencies": {
    "@types/crypto-js": "4.1.0",
    "@types/platform": "1.3.4",
    "@types/uuid": "8.3.4",
    "rimraf": "4.1.2",
    "tsup": "6.2.0",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@accelbyte/validator": "0.2.17",
    "axios": "1.3.6",
    "buffer": "6.0.3",
    "crypto-js": "4.1.1",
    "lodash": "4.17.21",
    "platform": "1.3.6",
    "uuid": "8.3.2",
    "validator": "13.7.0",
    "zod": "3.17.3"
  },
  "peerDependencies": {
    "@accelbyte/sdk": "^2.0.2"
  },
  "sideEffects": false
}