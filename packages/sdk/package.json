{
  "name": "@accelbyte/sdk",
  "version": "1.1.3",
  "author": "AccelByte Inc",
  "license": "AccelByte License",
  "main": "./dist/cjs/node/index.cjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/AccelByte/accelbyte-web-sdk.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/es/node/index.node.js",
        "default": "./dist/cjs/node/index.cjs"
      },
      "default": "./dist/es/browser/index.browser.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rollup -c rollup.config.mjs",
    "build:dts": "rollup -c rollup.dts.config.mjs",
    "postbuild": "yarn build:dts && rimraf dist/types",
    "test": "yarn test:unit",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "clean": "npx rimraf dist && npx rimraf node_modules"
  },
  "devDependencies": {
    "@playwright/test": "1.28.0",
    "@rollup/plugin-alias": "4.0.2",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-typescript": "11.0.0",
    "tslib": "2.4.0",
    "@types/crypto-js": "4.1.0",
    "@types/platform": "1.3.4",
    "@types/uuid": "8.3.4",
    "happy-dom": "8.2.6",
    "msw": "0.48.2",
    "rimraf": "4.1.2",
    "rollup": "3.8.1",
    "rollup-plugin-dts": "5.1.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "typescript": "4.6.4",
    "vitest": "0.25.1"
  },
  "dependencies": {
    "@accelbyte/validator": "0.2.0-beta.2",
    "axios": "0.27.2",
    "buffer": "6.0.3",
    "crypto-js": "4.1.1",
    "lodash": "4.17.21",
    "nanoid": "3.1.31",
    "platform": "1.3.6",
    "query-string": "7.1.1",
    "uuid": "8.3.2",
    "validator": "13.7.0",
    "zod": "3.17.3"
  },
  "sideEffects": false
}