{
  "name": "@accelbyte/sdk",
  "version": "0.1.1-alpha.52",
  "author": "AccelByte Inc",
  "license": "AccelByte License",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.es.js",
      "default": {
        "import": "./dist/index.es.js",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "__comment__prebuild__": "it builds 'packages/validator' as 'sdk' depends on it",
    "prebuild": "(cd ../validator && yarn build) && rimraf dist && (echo '---ls sdk:' && pwd && ls && echo '---ls root:' && cd .. && pwd && ls && echo '---ls validator:' && ls validator)",
    "doc": "typedoc --plugin typedoc-plugin-markdown --out docs src/api",
    "build": "rollup -c rollup.config.mjs",
    "build:dts": "rollup -c rollup.dts.config.mjs",
    "postbuild": "yarn build:dts && rimraf dist/types",
    "test": "vitest"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "4.0.2",
    "@rollup/plugin-commonjs": "20.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-typescript": "10.0.1",
    "@types/crypto-js": "4.1.0",
    "@types/platform": "1.3.4",
    "@types/uuid": "8.3.4",
    "msw": "0.48.2",
    "rimraf": "3.0.2",
    "rollup": "3.8.1",
    "rollup-plugin-dts": "5.1.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "typedoc": "0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "4.6.4",
    "vitest": "0.25.1"
  },
  "dependencies": {
    "axios": "0.27.2",
    "buffer": "6.0.3",
    "crypto-js": "4.1.1",
    "lodash-core": "4.17.19",
    "nanoid": "3.1.30",
    "platform": "1.3.6",
    "query-string": "7.1.1",
    "uuid": "8.3.2",
    "validator": "11.1.0",
    "zod": "3.17.3"
  }
}