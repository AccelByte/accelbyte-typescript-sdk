{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "dom",
      "ESNext"
    ],
    "moduleResolution": "node",
    "module": "CommonJS",
    "types": [
      "vite/client",
      "node"
    ],
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strict": true,
    "noUnusedLocals": true,
    "pretty": true,
    "skipLibCheck": true,
    "target": "ES6",
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "newLine": "crlf",
    "baseUrl": ".",
    "paths": {
      "@accelbyte/validator": [
        "../validator/src"
      ],
      "@accelbyte/error-translator": [
        "../error-translator/src"
      ],
      "@accelbyte/sdk/*": [
        "../sdk/src/*"
      ],
      "@accelbyte/sdk": [
        "../sdk/src"
      ],
      "@accelbyte/sdk-basic": [
        "../sdk-basic/src"
      ],
      "@accelbyte/sdk-buildinfo": [
        "../sdk-buildinfo/src"
      ],
      "@accelbyte/sdk-event": [
        "../sdk-event/src"
      ],
      "@accelbyte/sdk-gdpr": [
        "../sdk-gdpr/src"
      ],
      "@accelbyte/sdk-iam": [
        "../sdk-iam/src"
      ],
      "@accelbyte/sdk-legal": [
        "../sdk-legal/src"
      ],
      "@accelbyte/sdk-odinconfig": [
        "../sdk-odinconfig/src"
      ],
      "@accelbyte/sdk-platform": [
        "../sdk-platform/src"
      ],
      "@od-assets/*": [
        "../../../packages/od-assets/src/*"
      ],
      "@od-components/*": [
        "../../../packages/od-components/src/*"
      ],
      "@od-shared/*": [
        "../../../packages/od-shared/src/*"
      ],
      "seed/*": [
        "../../../packages/od-shared/seed/*"
      ],
      "@od-legal/*": [
        "../../../packages/od-legal/src/*"
      ],
      "@od-account/*": [
        "../../../packages/od-account/src/*"
      ],
      "@od-ecommerce/*": [
        "../../../packages/od-ecommerce/src/*"
      ],
      "@od-payment/*": [
        "../../../packages/od-payment/src/*"
      ],
      "@od-twitch/*": [
        "../../../packages/od-twitch/src/*"
      ],
      "@od-state/*": [
        "../../../packages/od-state/src/*"
      ],
      "@od-medium-serde": [
        "../../../packages/od-medium-serde/src"
      ]
    },
    "alwaysStrict": true,
    "declarationMap": true,
    "experimentalDecorators": true
  },
  "include": [
    "src",
    "types",
    "scripts"
  ],
  "exclude": [
    "*.cjs",
    "node_modules"
  ]
}