{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "module": "ESNext",
    "types": [
      "node"
    ],
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strict": true,
    "noUnusedLocals": true,
    "pretty": true,
    "skipLibCheck": true,
    "target": "es2021",
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": false,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "newLine": "crlf",
    "baseUrl": ".",
    "paths": {
      "@accelbyte/validator": [
        "../validator/src"
      ],
      "@accelbyte/error-translator": [
        "../error-translator/src"
      ],
      "@accelbyte/sdk/*": [
        "../sdk/src/*"
      ],
      "@accelbyte/sdk": [
        "../sdk/src"
      ],
      "@accelbyte/sdk-basic": [
        "../sdk-basic/src"
      ],
      "@accelbyte/sdk-buildinfo": [
        "../sdk-buildinfo/src"
      ],
      "@accelbyte/sdk-event": [
        "../sdk-event/src"
      ],
      "@accelbyte/sdk-gdpr": [
        "../sdk-gdpr/src"
      ],
      "@accelbyte/sdk-iam": [
        "../sdk-iam/src"
      ],
      "@accelbyte/sdk-legal": [
        "../sdk-legal/src"
      ],
      "@accelbyte/sdk-odinconfig": [
        "./src"
      ],
      "@accelbyte/sdk-platform": [
        "../sdk-platform/src"
      ],
      "~/iam-definitions/*": [
        "../sdk-iam/src/generated-public/iam/definitions/*"
      ]
    },
    "outDir": "dist/es"
  },
  "include": [
    "src",
    "test"
  ],
  "exclude": [
    "scripts",
    "node_modules",
    "dist"
  ]
}