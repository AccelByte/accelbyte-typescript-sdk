        "produces": ["application/json"],
        "tags": ["Templates"],
        "summary": "Create template",
        "operationId": "AdminTemplateCreate",
        "parameters": [
          {
            "type": "string",
            "description": "namespace of the user",
            "name": "namespace",
            "in": "path",
            "required": true
          },
          {
            "name": "body",
